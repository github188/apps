#!/bin/sh

export PATH=/usr/local/bin:$PATH

/usr/local/bin/md-assume.sh

nohup /usr/local/bin/us_d &

modprobe scst
modprobe scst_vdisk
modprobe iscsi-scst
/usr/local/sbin/iscsi-scstd

# add target
cd /sys/kernel/scst_tgt/targets/iscsi
echo "add_target iqn.2006-10.net.vlnb:tgt" > mgmt

export LANG=en_US.UTF-8
export LANGUAGE=en_US.UTF-8
export LC_ALL=en_US.UTF-8

# *** JW-NAS-CONF-START ***
# *** JW-NAS-CONF-END ***
